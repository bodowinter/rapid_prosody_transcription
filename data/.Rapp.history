## Load in data:#
#
setwd("/Users/teeniematlock/Desktop/research/mathematical_cognition/tiny_numbers/new_web_experiment/data2/")#
filenames = list.files()#
xdata = c()#
for(name in filenames){#
	xdata = rbind(xdata,read.table(name,sep=";",header=F))#
	}#
#
## Get rid off extra column:#
#
xdata = xdata[,-ncol(xdata)]#
#
## Give names:#
#
colnames(xdata) = c("condition","response","video_viewed","body_awareness")#
#
## How many couldn't view the video?#
#
table(xdata$video_viewed)			# 4 couldn't#
#
## Exclude those that couldn't view the video:#
#
xdata = xdata[xdata$video_viewed == "yes",]#
#
## Check weird responses#
#
xdata$response = as.numeric(xdata$response)#
#
## Get rid of NA's:#
#
xdata = xdata[!is.na(xdata$response),]#
#
## Check proportions:#
#
table(xdata$condition)#
#
## Most frequent response:#
#
table(xdata$response)#
#
## Make a boxplot:#
#
boxplot(response ~ condition,xdata,ylim=c(0,30))#
#
## Center body awareness variable:#
#
xdata$body_awareness_c = xdata$body_awareness-mean(xdata$body_awareness)#
xdata$body_awareness_z = scale(xdata$body_awareness)#
#
## Make a test (poisson regression makes sense here):#
#
xmdl = glm(response ~ condition*body_awareness_z,xdata,family="poisson")#
summary(xmdl)#
#
## Check body awareness results:#
#
table(xdata$body_awareness)#
mean(xdata$body_awareness)#
sd(xdata$body_awareness)#
#
## Check estimates:#
## with body awareness at mean:#
exp(coef(xmdl)["(Intercept)"])			# open#
exp(coef(xmdl)["(Intercept)"]+coef(xmdl)["conditionpinch"])		# pinch#
## with body awareness +1SD:#
exp(coef(xmdl)["(Intercept)"]+coef(xmdl)["body_awareness_z"])			# open#
exp(coef(xmdl)["(Intercept)"]+coef(xmdl)["conditionpinch"]+coef(xmdl)["conditionpinch:body_awareness_z"])		# pinch#
